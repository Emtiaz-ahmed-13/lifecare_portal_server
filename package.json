{
  "name": "lifecare_porter_server",
  "version": "1.0.0",
  "main": "server.js",
  "author": "lifecare_porter_server",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respwn --transpile-only ./src/server.ts",
    "db:generate": "prisma generate --schema= ./prisma/schema",
    "db:migrate": "prisma migrate dev --schema= ./prisma/schema",
    "db:push": "prisma db push --schema= ./prisma/schema",
    "db:studio": "prisma studio --schema= ./prisma/schema",
    "db:pull": "prisma db pull --schema= ./prisma/schema",
    "stripe:webhook":"stripe listen --forward-to localhost:5001/webhook",
    "postinstall":"prisma generate --schema= ./prisma/schema"
  },

  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@prisma/config": "^7.2.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.5",
    "@types/socket.io": "^3.0.1",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.9.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "http-status": "^2.1.0",
    "i": "^0.3.7",
    "install": "^0.13.0",
    "json-web-token": "^3.2.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "openai": "^6.16.0",
    "pg": "^8.17.1",
    "prisma": "^7.2.0",
    "socket.io": "^4.8.3",
    "stripe": "^20.2.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.0",
    "@types/pg": "^8.11.10",
    "@types/ws": "^8.5.14",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
